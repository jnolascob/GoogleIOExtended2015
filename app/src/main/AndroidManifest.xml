<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.herovitamin.androiddesignsupportlibraryapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationActivity"
            android:label="@string/title_activity_navigation" >
        </activity>
        <activity
            android:name=".HumbleImprovementsActivity"
            android:label="@string/title_activity_humble_improvements" >
        </activity>
        <activity
            android:name=".SnackbarActivity"
            android:label="@string/title_activity_snackbar" >
        </activity>
        <activity
            android:name=".CoordinatorLayoutAndAppBarActivity"
            android:label="@string/title_activity_coordinator_layout_and_fab" >
        </activity>
        <activity
            android:name=".CoordinatorAppBarWithTabsActivity"
            android:label="@string/title_activity_coordinator_app_bar_with_tabs" >
        </activity>
        <activity
            android:name=".CoordinatorAndFABActivity"
            android:label="@string/title_activity_coordinator_and_fab" >
        </activity>
        <activity
            android:name=".CollapsingToolBarLayoutActivity"
            android:label="@string/title_activity_collapsing_tool_bar_layout" >
        </activity>
        <activity
            android:name=".CollapsingToolbarAndFABActivity"
            android:label="@string/title_activity_collapsing_toolbar_and_fab" >
        </activity>
    </application>

</manifest>
